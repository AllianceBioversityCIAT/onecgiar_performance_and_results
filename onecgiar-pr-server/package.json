{
  "name": "onecgiar-pr-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose false --config ./test/jest-e2e.json --detectOpenHandles",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:empty": "cross-var npm run typeorm migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm migration:generate -- ./src/migrations/$npm_config_name -d ./src/config/orm.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/orm.config.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/orm.config.ts",
    "lambda:test": "sls offline start",
    "lambda:deploy": "sls deploy -v"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.1",
    "@types/jsonwebtoken": "^9.0.5",
    "activedirectory": "^0.7.2",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1499.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "dynamoose": "^3.2.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.3",
    "nestjs-dynamoose": "^0.5.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdf-creator-node": "^2.3.5",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/aws-lambda": "^8.10.126",
    "@types/aws-serverless-express": "^3.3.8",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.9.2",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.1.0",
    "serverless-offline": "^13.3.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.ts": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "overrides": {
    "json5": "^2.2.2",
    "tough-cookie": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/migrations/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/migrations/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 30,
        "lines": 35,
        "statements": 40
      }
    },
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
